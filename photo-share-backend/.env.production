# 环境配置
NODE_ENV=production
PORT=3000

# 数据库配置（生产环境使用 MySQL）
DB_TYPE=mysql
DB_NAME=photo_share
DB_USER=photo_share_user
DB_PASSWORD=your_strong_password
DB_HOST=localhost
DB_PORT=3306

# JWT配置
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 文件上传配置
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# CORS配置
CORS_ORIGIN=https://your_domain.com